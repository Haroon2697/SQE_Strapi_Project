{
  "name": "strapi-app",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application",
  "scripts": {
    "build": "strapi build && node scripts/copy-configs.js",
    "build:ci": "strapi build && node scripts/copy-configs.js || echo 'Build failed, but continuing...'",
    "build:ci": "strapi build || echo 'Build failed, but continuing...'",
    "console": "strapi console",
    "deploy": "strapi deploy",
    "dev": "strapi develop",
    "develop": "node scripts/strapi-develop.js",
    "start": "strapi start",
    "start:ci": "mkdir -p .tmp && NODE_ENV=test STRAPI_TELEMETRY_DISABLED=true npx strapi start",
    "test": "npm run test:ci && npm run test:e2e",
    "test:ci": "npm run test:unit && npm run test:e2e",
    "test:watch": "jest --watch",
    "test:unit": "jest --config=jest.config.js",
    "test:integration": "jest tests/integration",
    "test:coverage": "jest --coverage --config=jest.config.js",
    "test:e2e": "cypress run --browser firefox --headless",
    "test:e2e:open": "cypress open --e2e -b firefox",
    "strapi": "strapi",
    "upgrade": "npx @strapi/upgrade latest",
    "upgrade:dry": "npx @strapi/upgrade latest --dry"
  },
  "overrides": {
    "glob": "^10.0.0",
    "rimraf": "^5.0.0",
    "inflight": "^1.0.6",
    "tmp": "^0.2.1",
    "esbuild": "^0.24.2"
  },
  "dependencies": {
    "@strapi/plugin-cloud": "5.31.2",
    "@strapi/plugin-users-permissions": "5.31.2",
    "@strapi/strapi": "5.31.2",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.6.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "babel-jest": "^30.2.0",
    "cross-env": "^10.1.0",
    "cypress": "^15.7.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "supertest": "^7.1.4",
    "typescript": "^5",
    "wait-on": "^7.2.0"
  },
  "engines": {
    "node": ">=20.0.0 <=24.x.x",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "uuid": "bb79cb17-9d26-4f39-9f58-e67a7582cd29",
    "installId": "d410392be2b5107fca83f1eded467eb08a559a16b274c66919dc01c10142340f"
  }
}
